                        <li class="nav-item col-6 col-lg-auto">
                            @Html.ActionLink("Borrar", "Eliminar", "Empleado", new { area = "" }, new { @class = "nav-link py-2 px-0 px-lg-2" })
                        <li class="nav-item col-6 col-lg-auto">
                            @Html.ActionLink("Insertar", "Guardar", "Empleado", new { area = "" }, new { @class = "nav-link py-2 px-0 px-lg-2" })
